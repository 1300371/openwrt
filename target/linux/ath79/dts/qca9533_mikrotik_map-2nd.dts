/dts-v1/;

#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>

#include "qca953x.dtsi"

/ {
	compatible = "mikrotik,map-2nd", "qca,qca9533";
	model = "MikroTik RBmAP2nD";

	chosen {
		bootargs = "console=ttyS0,115200n8";
	};

	aliases {
		led-boot = &user;
		led-failsafe = &user;
		led-running = &user;
		led-upgrade = &user;
		label-mac-device = &eth0;
	};

	leds {
		compatible = "gpio-leds";

		power {
			label = "map-2nd:green:power";
			gpios = <&gpio 4 GPIO_ACTIVE_LOW>;
			default-state = "on";
			panic-indicator;
		};

		lan1 {
			label = "map-2nd:green:lan1";
			gpios = <&ssr 0 GPIO_ACTIVE_LOW>;
		};

		lan2 {
			label = "map-2nd:green:lan2";
			gpios = <&ssr 1 GPIO_ACTIVE_LOW>;
		};

		poe {
			label = "map-2nd:green:poe";
			gpios = <&ssr 2 GPIO_ACTIVE_LOW>;
		};

		user: user {
			label = "map-2nd:green:user";
			gpios = <&ssr 3 GPIO_ACTIVE_LOW>;
		};

		wlan {
			label = "map-2nd:green:wlan";
			gpios = <&ssr 4 GPIO_ACTIVE_LOW>;
			linux,default-trigger = "phy0tpt";
		};

		apcap {
			label = "map-2nd:green:apcap";
			gpios = <&ssr 6 GPIO_ACTIVE_LOW>;
		};
	};

	keys {
		compatible = "gpio-keys";

		reset_button {
			label = "Reset button";
			linux,code = <KEY_RESTART>;
			gpios = <&gpio 16 GPIO_ACTIVE_LOW>;
		};
	};

	gpio-export {
		compatible = "gpio-export";

		poe_passtrough {
			gpio-export,name = "map-2nd:poe_passtrough";
			gpio-export,output = <0>;
			gpios = <&gpio 14 GPIO_ACTIVE_HIGH>;
		};
	};
};

&uart {
	status = "okay";
};

&spi {
	status = "okay";
	num-cs = <2>;
	cs-gpios = <0>, <&gpio 11 GPIO_ACTIVE_LOW>;

	flash@0 {
		#address-cells = <1>;
		#size-cells = <1>;
		compatible = "jedec,spi-nor";
		reg = <0>;
		spi-max-frequency = <25000000>;

		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			partition@0 {
				label = "routerboot";
				reg = <0x000000 0xe000>;
				read-only;
			};

			hard_config: partition@e000 {
				label = "hard_config";
				reg = <0xe000 0x1000>;
				read-only;
			};

			partition@f000 {
				label = "bios";
				reg = <0xf000 0x1000>;
				read-only;
			};

			partition@10000 {
				label = "routerboot2";
				reg = <0x10000 0xf000>;
				read-only;
			};

			partition@1f000 {
				label = "soft_config";
				reg = <0x1f000 0x1000>;
			};

			partition@20000 {
				label = "firmware";
				reg = <0x20000 0xfe0000>;
				compatible = "mikrotik,minor";
			};
		};
	};

	ssr: ssr@1 {
		compatible = "fairchild,74hc595";
		reg = <1>;
		gpio-controller;
		#gpio-cells = <2>;
		registers-number = <1>;

		usb_power {
				gpio-hog;
				gpios = <5 GPIO_ACTIVE_LOW>;
				output-high;
				line-name = "usb_power";
		};
	};
};

&usb0 {
	status = "okay";
};

&usb_phy {
	status = "okay";
};

&eth0 {
	status = "okay";
	phy-handle = <&swphy4>;
	mtd-mac-address = <&hard_config 0x10>;
};

&eth1 {
	status = "okay";
};

&wmac {
	status = "okay";
	mtd-mac-address = <&hard_config 0x10>;
	mtd-mac-address-increment = <2>;
	qca,no-eeprom;
};
