/dts-v1/;

#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>

#include "qca9533.dtsi"

/ {
	compatible = "mikrotik,rb750p-pbr2", "qca,qca9533";
	model = "Mikrotik RouterBOARD RB750P-PBr2 (Powerbox)";

	chosen {
		bootargs = "console=ttyS0,115200n8";
	};

	aliases {
		led-status = &user;
	};

	leds {
		compatible = "gpio-leds";
		
		user: user {
			label = "rb:green:user";
			gpios = <&gpio 4 GPIO_ACTIVE_HIGH>;
			default-state = "on";
		};

		lan1 {
			label = "rb:green:port1";
			gpios = <&ssr 0 GPIO_ACTIVE_LOW>;
			default-state = "off";
		};

		lan2 {
			label = "rb:green:port2";
			gpios = <&ssr 1 GPIO_ACTIVE_LOW>;
			default-state = "off";
		};

		lan3 {
			label = "rb:green:port3";
			gpios = <&ssr 2 GPIO_ACTIVE_LOW>;
			default-state = "off";
		};

		lan4 {
			label = "rb:green:port4";
			gpios = <&ssr 3 GPIO_ACTIVE_LOW>;
			default-state = "off";
		};

		lan5 {
			label = "rb:green:port5";
			gpios = <&ssr 4 GPIO_ACTIVE_LOW>;
			default-state = "off";
		};
	};

	keys {
		compatible = "gpio-keys";
		#address-cells = <1>;
		#size-cells = <0>;

		reset {
			label = "Reset button";
			linux,code = <KEY_RESTART>;
			gpios = <&gpio 16 GPIO_ACTIVE_LOW>;
			debounce-interval = <60>;
		};
	};

	gpio-export {
		compatible = "gpio-export";
		#size-cells = <0>;

		gpio_poe_power {
			gpio-export,name = "rb:power:poe";
			gpio-export,output = <0>;
			gpios = <&gpio 14 GPIO_ACTIVE_HIGH>;
		};
	};
};

&uart {
	status = "okay";
};

&spi {
	status = "okay";
	num-cs = <2>;
	cs-gpios = <0>, <&gpio 11 0>

	flash@0 {
		#address-cells = <1>;
		#size-cells = <1>;
		compatible = "jedec,spi-nor";
		reg = <0>;
		spi-max-frequency = <25000000>;
	};

	ssr: hch595@1 {
		compatible = "fairchild,74hc595";
		reg = <1>;
		gpio-controller;
		#gpio-cells = <2>;
		registers-number = <1>;
		spi-max-frequency = <25000000>;
	};
};

&mdio0 {
	status = "okay";

	phy4: ethernet-phy@4 {
		reg = <4>;
		phy-mode = "mii";
	};
};

&eth0 {
	status = "okay";

	//mtd-mac-address = <&art 0x0>;
	phy-handle = <&phy4>;
	phy-mode = "mii";
};

&eth1 {
	status = "okay";

	//mtd-mac-address = <&art 0x6>;
	phy-mode = "gmii";

	fixed-link {
		speed = <1000>;
		full-duplex;
	};
};
